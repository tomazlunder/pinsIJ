fun whileFun(x:integer, y:integer):integer = (
    {a = x},
    {while (a <= y) : (putInt(a), {a = a+1})},
    0)
    {where var a : integer};

fun forFun(x:integer, y:integer):integer = (
    {for a = x, y, 1 : putInt(a)},
    1)
    {where var a : integer};

fun main(x:integer):integer = (
    whileFun(1,5),
    forFun(1,5),
    1)